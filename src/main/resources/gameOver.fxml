<AnchorPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.GameOverController">
   <children>
      
         <GridPane fx:id="gameOverScreen" layoutX="200.0" layoutY="192.0" AnchorPane.bottomAnchor="175.0" AnchorPane.leftAnchor="175.0" AnchorPane.rightAnchor="175.0" AnchorPane.topAnchor="175.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="gameOver" text="Game Over" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
               <Button fx:id="restartButton" mnemonicParsing="false" text="Restart Game" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="TOP" />
               <Button fx:id="quitButton" mnemonicParsing="false" text="Quit" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="TOP" />
               <ImageView fx:id="gameOverView" fitHeight="344.0" fitWidth="525.0" pickOnBounds="true" preserveRatio="true" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                  <image>
                     <Image url="@gameOverImage.png" />
                  </image>
               </ImageView>
            </children>
         </GridPane>
   </children>
  
	<stylesheets>
        <URL value="@stylesheet.css" />
    </stylesheets>
    
</AnchorPane>
